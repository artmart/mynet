{
    "name": "symfony/framework-standard-edition",
    "description": "The \"Symfony Standard Edition\" distribution",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "2.1.8",
        "doctrine/orm": ">=2.2.3,<2.4-dev",
        "doctrine/doctrine-bundle": "1.0.*",
        "twig/extensions": "1.0.*",
        "symfony/assetic-bundle": "2.1.*",
        "symfony/swiftmailer-bundle": "2.1.*",
        "symfony/monolog-bundle": "2.1.*",
        "sensio/distribution-bundle": "2.1.*",
        "sensio/framework-extra-bundle": "2.1.*",
        "sensio/generator-bundle": "2.1.*",
        "jms/security-extra-bundle": "1.2.*",
        "jms/di-extra-bundle": "1.1.*",
        
        "friendsofsymfony/user-bundle": "1.3.1",
        
        "sonata-project/admin-bundle": "2.1.*@dev",
        "sonata-project/user-bundle": "2.1.*@dev",
        "sonata-project/doctrine-orm-admin-bundle": "2.1.*@dev",
        "sonata-project/cache-bundle": "2.1.*@dev",
        "sonata-project/block-bundle": "2.1.*@dev",
        
        "sonata-project/media-bundle": "2.1.*@dev",
        
        
        "sonata-project/easy-extends-bundle": "dev-master",
        
        "rodchyn/cron-manager-bundle": "dev-master",
        "a2lix/translation-form-bundle" : "0.*@dev",
        "stfalcon/tinymce-bundle": "dev-master",
        "genemu/form-bundle": "2.1.x-dev",
        
        "knplabs/knp-paginator-bundle": "v2.2",
        "gedmo/doctrine-extensions": "dev-master",
        "stof/doctrine-extensions-bundle": "dev-master",
        "presta/sitemap-bundle": "dev-master",
        "doctrine/doctrine-migrations-bundle": "dev-master",
        "Trsteel/ckeditor-bundle": "dev-master#429291f084222f8d698e85e6446d15ac6f1021ea",
        "hwi/oauth-bundle": "0.3.*@dev"
        
    },
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web"
    }
}
